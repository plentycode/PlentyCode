{
  "name": "PlentyCode-Splash",
  "version": "2.0.1",
  "description": "Splash page for plentycode.com site",
  "main": "index.js",
  "private": true,
  "dependencies": {
    "browserify": "^9.0.3",
    "gulp": "^3.8.11",
    "gulp-sourcemaps": "^1.5.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "devDependencies": {
    "angular-route": "^1.3.15",
    "gulp-bower": "0.0.10",
    "gulp-concat": "^2.5.2",
    "gulp-less": "^3.0.2",
    "gulp-uglify": "^1.1.0",
    "stringify": "^3.1.0"
  },
  "browserify": {
    "transform": [
      [
        "stringify",
        "browserify-shim"
      ]
    ],
    "extensions": [
      ".html",
      ".json"
    ],
    "insertGlobals": true,
    "detectGlobals": true,
    "debug": true
  },
  "browser": {
    "angular": "./js/vendor/angular/angular.js",
    "angular-mocks": "./js/vendor/angular-mocks/angular-mocks.js",
    "ngAnimate": "./js/vendor/angular-animate/angular-animate.js"
  },
  "browserify-shim": {
    "angular": "angular",
    "lodash": "_",
    "angular-mocks": "angular-mocks",
    "ngAnimate": {
      "exports": "ngAnimate",
      "depends": [
        "angular"
      ]
    }
  },
  "scripts": {},
  "author": "Carlos I Mercado",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/civanm/PlentyCode.git"
  },
  "bugs": {
    "url": "https://github.com/civanm/PlentyCode/issues"
  },
  "homepage": "https://github.com/civanm/PlentyCode"
}
