{
  "name": "Plentycode.com",
  "version": "2.0.0",
  "homepage": "https://github.com/civanm/PlentyCode",
  "authors": [
    "Carlos Iv√°n Mercado <carlos.mercado@plentycode.com>"
  ],
  "description": "Vendors external libs",
  "main": "index.js",
  "moduleType": [],
  "license": "MIT",
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "angular": "~1.3.15",
    "angular-mocks": "~1.3.15",
    "requirejs": "~2.1.16",
    "jquery": "~2.1.3",
    "angular-animate": "~1.3.15",
    "clearless": "*",
    "angular-ui-router": "~0.2.13",
    "angular-route": "~1.3.15"
  },
  "exportsOverride": {
    "clearless": {
      "less": "mixins/*.less"
    },
    "jquery": {
      "js": "dist/jquery.js"
    },
    "jquery-ui": {
      "js": "ui/widget.js"
    },
    "jquery.easing": {
      "js": "js/jquery.easing.js"
    },
    "jquery.selectBoxIt": {
      "js": "src/javascripts/jquery.selectBoxIt.js"
    },
    "jquery.dotdotdot": {
      "js": "src/js/jquery.dotdotdot.js"
    },
    "modernizr": {
      "js": "modernizr.js",
      "js/modernizr/feature-detects/": [
        "feature-detects/*.js"
      ]
    },
    "html5shiv": {
      "js": "dist/html5shiv.js"
    },
    "angular": {
      "js": "angular.js"
    },
    "angular-mocks": {
      "js": "angular-mocks.js"
    },
    "angular-ui-router": {
      "js": "releases/angular-ui-router.js"
    },
    "*": {
      "js": "*.js"
    }
  }
}
